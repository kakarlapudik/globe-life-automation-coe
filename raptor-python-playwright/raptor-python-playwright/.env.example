# RAPTOR Framework - Environment Variables Example
# Copy this file to .env and fill in your actual values

# Browser Configuration
RAPTOR_BROWSER_TYPE=chromium
RAPTOR_HEADLESS=false

# Timeout Configuration (in milliseconds)
RAPTOR_DEFAULT_TIMEOUT=20000

# Database Configuration
RAPTOR_DB_HOST=localhost
RAPTOR_DB_PORT=1433
RAPTOR_DB_USER=your_username
RAPTOR_DB_PASSWORD=your_password
RAPTOR_DB_NAME=DDFE_Dev

# ALM Configuration
RAPTOR_ALM_URL=https://alm.example.com
RAPTOR_ALM_USERNAME=your_alm_username
RAPTOR_ALM_PASSWORD=your_alm_password
RAPTOR_ALM_DOMAIN=YOUR_DOMAIN
RAPTOR_ALM_PROJECT=YOUR_PROJECT

# JIRA Configuration
RAPTOR_JIRA_URL=https://jira.example.com
RAPTOR_JIRA_USERNAME=your_jira_username
RAPTOR_JIRA_API_TOKEN=your_jira_api_token
RAPTOR_JIRA_PROJECT_KEY=RAPTOR

# Environment Selection
RAPTOR_ENVIRONMENT=dev
